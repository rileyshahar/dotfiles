colors cac

unbind >>
unbind <<
bind N tabpush
bind h tabprev
bind l tabnext
bind H back
bind L forward
bind --mode=input <c-n> focusinput -n 
bind --mode=input <c-p> focusinput -N 

set editorcmd kitty nvim %f '+normal!%lGzv%c|'
set smoothscroll true

set searchurls.r https://docs.rs/%s/latest
set searchurls.yt https://www.youtube.com/results?search_query=%s
set searchurls.wiki https://en.wikipedia.org/wiki/%s
